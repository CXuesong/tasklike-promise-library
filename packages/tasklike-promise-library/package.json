{
  "name": "tasklike-promise-library",
  "version": "0.0.12",
  "description": "A .NET-Task-like Promise extension library for JavaScript.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.json && tsc --build tsconfig.cjs.json",
    "lint": "tslint -p .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc --watch -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CXuesong/tasklike-promise-library.git"
  },
  "keywords": [
    "tpl",
    "promise",
    "cancellation",
    "xhr"
  ],
  "author": "CXuesong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CXuesong/tasklike-promise-library/issues"
  },
  "homepage": "https://github.com/CXuesong/tasklike-promise-library#readme",
  "private": false,
  "devDependencies": {
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "tslib": "^1.11.1"
  },
  "browser": {
    "./dist/cjs/node.js": false,
    "./dist/esm/node.js": false
  }
}
